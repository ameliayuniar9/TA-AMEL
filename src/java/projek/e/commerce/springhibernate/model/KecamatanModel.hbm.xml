<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="projek.e.commerce.springhibernate.model.KecamatanModel" table="tb_kecamatan">
        <id column="id_kecamatan" name="id_kecamatan" type="string">
            <generator class="assigned"/>
        </id>
        <property column="id_kota" name="id_kota"/>
        <property column="nama" name="nama"/>
    </class>
</hibernate-mapping>
