<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="projek.e.commerce.springhibernate.model.OngkirModel" table="tb_ongkir">
        <id column="kode_ongkir" name="kode_ongkir" type="string">
            <generator class="assigned"/>
        </id>
        <property column="nama_kota" name="nama_kota"/>
        <property column="harga" name="harga"/>
    </class>
</hibernate-mapping>
