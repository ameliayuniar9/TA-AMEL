<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="projek.e.commerce.springhibernate.model.LoginModel" table="tb_login">
    <id column="id_login" name="id_login" type="string">
      <generator class="assigned"/>
    </id>
    <property column="username" name="username"/>
    <property column="password" name="password"/>
    <property column="akses" name="akses"/>
    <property column="id_pembeli" name="id_pembeli"/>
    <property column="nama_lengkap" name="nama_lengkap"/>
  </class>
</hibernate-mapping>
