<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="projek.e.commerce.springhibernate.model.DetailPesananModel" table="tb_detail_pesanan">
    <id column="kode_detail_pesanan" name="kode_detail_pesanan" type="string">
      <generator class="assigned"/>
    </id>
    <property column="kode_pesanan" name="kode_pesanan"/>
    <property column="kode_detail" name="kode_detail"/>
    <property column="kuantitas" name="kuantitas"/>
    <property column="total" name="total"/>
  </class>
</hibernate-mapping>
