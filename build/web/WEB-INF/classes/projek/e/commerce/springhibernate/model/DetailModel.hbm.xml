<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
<class name="projek.e.commerce.springhibernate.model.DetailModel" table="tb_detail">
    <id column="kode_detail" name="kode_detail" type="string">
      <generator class="assigned"/>
    </id>
    <property column="kode_produk" name="kode_produk"/>
    <property column="warna" name="warna"/>
    <property column="gambar" name="gambar"/>
    <property column="stok" name="stok"/>
  </class>
</hibernate-mapping>
