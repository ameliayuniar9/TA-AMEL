<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class name="projek.e.commerce.springhibernate.model.UlasanModel" table="tb_ulasan">
    <id column="kode_ulasan" name="kode_ulasan" type="string">
      <generator class="assigned"/>
    </id>
    <property column="video" name="video"/>
    <property column="keterangan" name="keterangan"/>
  </class>
</hibernate-mapping>
