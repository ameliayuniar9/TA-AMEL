<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="projek.e.commerce.springhibernate.model.PesananModel" table="tb_pesanan">
    <id column="kode_pesanan" name="kode_pesanan" type="string">
      <generator class="assigned"/>
    </id>
    <property column="id_pembeli" name="id_pembeli"/>
    <property column="total_pesanan" name="total_pesanan"/>
    <property column="tanggal_pesan" name="tanggal_pesan"/>
    <property column="status" name="status"/>
    <property column="bukti_pembayaran" name="bukti_pembayaran"/>
    <property column="id_penerima" name="id_penerima"/>
    <property column="tanggal_pembayaran" name="tanggal_pembayaran"/>
  </class>
</hibernate-mapping>
